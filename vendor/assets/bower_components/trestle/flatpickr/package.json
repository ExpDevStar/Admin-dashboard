{
  "name": "flatpickr",
  "version": "3.0.7",
  "description": "A lightweight, powerful javascript datetime picker",
  "scripts": {
    "start": "nodemon --watch build.js build.js -- --dev",
    "build": "node build.js",
    "lint": "eslint src/",
    "test": "npm run lint && jest --bail && npm run test-ssr",
    "test-ssr": "node -e 'require(\"./src/flatpickr.js\")'",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "release": "jest --bail --silent && ./release"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-stylus": "latest",
    "babel-cli": "latest",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-assign": "latest",
    "babel-plugin-transform-remove-strict-mode": "latest",
    "babel-preset-es2015": "latest",
    "chokidar": "latest",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-plugin-jest": "^20.0.3",
    "glob": "latest",
    "http-server": "latest",
    "jest": "latest",
    "livereload": "latest",
    "ncp": "latest",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "opn": "latest",
    "rtlcss": "latest",
    "semver": "latest",
    "stylus": "latest",
    "uglify-js": "latest"
  },
  "main": "dist/flatpickr.js",
  "style": "dist/flatpickr.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chmln/flatpickr.git"
  },
  "author": "Gregory <gregory.mkv@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chmln/flatpickr/issues"
  },
  "homepage": "https://chmln.github.io/flatpickr",
  "keywords": [
    "javascript",
    "datetimepicker",
    "calendar",
    "date",
    "time",
    "picker",
    "lightweight"
  ],
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/flatpickr",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "browserslist": [
    "ie >= 9",
    "last 2 versions",
    "safari >= 7"
  ]
}
