@font-face {
  font-family: 'Font Awesome 5 Free';
  font-style: normal;
  font-weight: 400;
  src: url(<%= asset_path('trestle/fa-regular-400.eot') %>);
  src: url(<%= asset_path('trestle/fa-regular-400.eot?#iefix') %>) format('embedded-opentype'),
       url(<%= asset_path('trestle/fa-regular-400.woff2') %>) format('woff2'),
       url(<%= asset_path('trestle/fa-regular-400.woff') %>) format('woff'),
       url(<%= asset_path('trestle/fa-regular-400.ttf') %>) format('truetype'),
       url(<%= asset_path('trestle/fa-regular-400.svg#fontawesome') %>) format('svg');
}

